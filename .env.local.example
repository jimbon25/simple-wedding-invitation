# Copy this file to .env.local, .env.vercel, or .env.netlify and fill in the values

# Public variables (available to browser)
REACT_APP_RECAPTCHA_SITE_KEY=your_recaptcha_site_key
REACT_APP_GUEST_API_KEY=your_guest_api_key
REACT_APP_SITE_URL=https://your-site-url.netlify.app

# For Vercel (use these instead of REACT_APP_ variables)
NEXT_PUBLIC_RECAPTCHA_SITE_KEY=your_recaptcha_site_key
NEXT_PUBLIC_GUEST_API_KEY=your_guest_api_key
NEXT_PUBLIC_SITE_URL=https://your-site-url.vercel.app

# Private variables (server only)

# reCAPTCHA
RECAPTCHA_SECRET_KEY=your_recaptcha_secret_key

# Telegram notification bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# Discord notification webhook
DISCORD_WEBHOOK_URL=your_discord_webhook_url

# For Analytics log
ANALYTICS_BOT_TOKEN=your_analytics_bot_token
ANALYTICS_CHAT_ID=your_analytics_chat_id

# For Suspect log
SUSPECT_BOT_TOKEN=your_suspect_bot_token # Separate bot for suspect detection
SUSPECT_CHAT_ID=your_suspect_chat_id

# Gemini AI
GEMINI_API_KEY=your_gemini_api_key 

# IP Information
IPINFO_TOKEN=your_ipinfo_token
ALLOWED_COUNTRIES=your_allowed_countries # Comma-separated list of allowed countries (e.g., "US,CA,GB")

# Authentication
GUEST_API_KEY=your_guest_api_key # for guest access only you can change this key with your own

# Pentest
PENTEST_RATE_KEY=your_pentest_rate_key # set if you want to enable pentest features bypass limitations
# same as GUEST_API_KEY, you can change this key with your own
# It is used for admin and pentest only.
# Ensure this is not included in public builds or client-side code.
# If you are using Vercel or Netlify, set this in their environment variable settings

